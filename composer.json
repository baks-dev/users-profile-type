{
  "name": "baks-dev/users-profile-type",
  "description": "Модуль типов профилей пользователя",
  "homepage": "https://github.com/baks-dev/users-profile-type",
  "keywords": [
    "symfony",
    "bundle",
    "library",
    "module",
    "plugin",
    "user",
    "profile",
    "baksdev"
  ],
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Baks Development",
      "email": "admin@baks.dev"
    }
  ],
  "require": {
    "php": ">=8.4",
    "baks-dev/core": "^7.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/test-pack": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "BaksDev\\Users\\Profile\\TypeProfile\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BaksDev\\Users\\Profile\\TypeProfile\\Tests\\": "Tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "baks:users-profile-type:user": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ]
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}
